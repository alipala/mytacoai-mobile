{
  "expo": {
    "name": "MyTaco",
    "slug": "MyTacoAIMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4FD1C5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bigdavinci.mytaco",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to enable voice conversations for language practice.",
        "NSCameraUsageDescription": "This app may need access to your camera for future video features.",
        "NSUserNotificationsUsageDescription": "This app uses notifications to send you important updates and reminders about your language learning progress.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.bigdavinci.mytaco",
              "mytacoai",
              "com.googleusercontent.apps.41687548204-2gm0vhfjqub78lm5pqd7qh5drj6vmmrb"
            ]
          }
        ]
      }
    },
    "scheme": "mytacoai",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bigdavinci.MyTacoAI",
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "MODIFY_AUDIO_SETTINGS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "mytacoai"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d632af3a-3353-4613-a039-4b98411bbcda"
      }
    }
  }
}